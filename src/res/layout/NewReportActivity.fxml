<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.NewReportActivity"
            prefHeight="400.0" prefWidth="600.0">
    <Label layoutX="265.0" layoutY="25.0" text="New Report">
        <font>
            <Font size="25.0" />
        </font></Label>
    <Label layoutX="67.0" layoutY="100.0" text="-Date:">
        <font>
            <Font size="15.0" />
        </font></Label>
    <DatePicker fx:id="dateFill" layoutX="245.0" layoutY="100.0" prefHeight="20.0" prefWidth="300.0" promptText="date" />
    <Label layoutX="67.0" layoutY="180.0" text="-Zip:">
        <font>
            <Font size="15.0" />
        </font></Label>
    <TextField fx:id="zipFill" layoutX="245.0" layoutY="180.0" promptText="ex.11237.0" prefWidth="300"/>
    <Label layoutX="67.0" layoutY="260.0" text="-Address:">
        <font>
            <Font size="15.0" />
        </font></Label>
    <TextArea fx:id="addressFill" layoutX="245.0" layoutY="260.0" promptText="ex.112 NORTH STREET" prefWidth="300" prefHeight="50"/>
    <Label layoutX="67.0" layoutY="340.0" text="-City:">
        <font>
            <Font size="15.0" />
        </font></Label>
    <TextField fx:id="boroughFill" layoutX="245.0" layoutY="340.0" promptText="ex.BROOKLYN" prefWidth="300"/>
    <Label layoutX="67.0" layoutY="420.0" text="-Location Type:">
        <font>
            <Font size="15.0" />
        </font></Label>
    <TextField fx:id="typeFill" layoutX="245.0" layoutY="420.0" promptText="ex.3+ Family Apt. Building" prefWidth="300"/>
    <Button layoutX="117.0" layoutY="480.0" mnemonicParsing="false" onAction="#submitNewReport" text="Submit">
        <font>
            <Font size="30.0" />
        </font></Button>
    <Button layoutX="354.0" layoutY="480.0" mnemonicParsing="false" onAction="#backToMap" text="Back">
        <font>
            <Font size="30.0" />
        </font></Button>
</AnchorPane>
